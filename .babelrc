{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "ignore": [
    "node_modules/**"
  ],
  "plugins": ["@babel/plugin-proposal-export-default-from", "@babel/plugin-proposal-export-namespace-from",
    ["@babel/plugin-transform-runtime",  {
    "helpers": false
  }], "@babel/plugin-proposal-class-properties"]
}
