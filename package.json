{
  "name": "byj-ui",
  "version": "0.0.0-development",
  "description": "rollup+webpack ui component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "publish": "npm login && npm publish",
    "start": "cp -r lib example/src && cp -r es example/src && cd example && yarn && yarn start",
    "build": "rm -rf lib && rm -rf es && npx rollup -c && yarn lib && yarn es && yarn build:static",
    "codecov": "npx codecov",
    "lib": "babel src/index.js --presets=@babel/preset-env  --out-file lib/index.js",
    "es": "babel src/index.js --out-file es/index.js",
    "gc": "commit",
    "lint": "npx eslint --ext .js,.jsx .",
    "prepare": "npm run build",
    "semantic-release": "npx semantic-release",
    "build:static": "webpack --config ./config/webpack.config.js --mode production && rm -rf ./lib/style/js && node ./config/util.js",
    "storybook": "npx start-storybook -p 6006",
    "test": "npx jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/someOneJYB/byj-ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rollup",
    "react",
    "react component",
    "rollup react",
    "component",
    "example react component",
    "example rollup react component"
  ],
  "author": "byj",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.9.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@commitlint/prompt-cli": "^8.3.5",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-centered": "^5.3.18",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-options": "^5.3.18",
    "@storybook/channels": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@storybook/storybook-deployer": "^2.8.5",
    "acorn": "^7.1.1",
    "autoprefixer": "^9.7.6",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "^0.5.1",
    "canvas": "^2.6.1",
    "codecov": "^3.6.5",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "es-check": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "fibers": "^5.0.0",
    "husky": "^4.2.5",
    "jest-transform-css": "^2.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "marked": "^1.0.0",
    "mini-css-extract-plugin": "^0.9.0",
